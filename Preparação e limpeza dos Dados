p\n","\n","# Criando uma DF como teste\n","df_salarios = pd.DataFrame ({\n","      'nome': [\"Ana\", \"Bruno\", \"Carlos\", \"Daniele\", \"Gustavo\"],\n","      'salario': [20000, np.nan, 5000, np.nan, 100000]\n","})\n","\n","#Calcula a mÃ©dia salarial e substitui os nulos pela a media arredondada\n","df_salarios['salario_media'] = df_salarios['salario'].fillna(df_salarios['salario'].mean().round(2))\n","\n","#Calcula a mediana salaria e substitui os nulos\n","df_salarios['salario_mediana'] = df_salarios['salario'].fillna(df_salarios['salario'].median())\n","\n","df_salarios"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"XVH-pnaIdPII","executionInfo":{"status":"ok","timestamp":1754587469045,"user_tz":180,"elapsed":96,"user":{"displayName":"Pedro Henrique","userId":"15352631098231293100"}},"outputId":"3bd63ce6-cbfc-4b3a-a3a5-e5632a1ed7a2"},"execution_count":13,"outputs":[{"output_type":"execute_result","data":{"text/plain":["      nome   salario  salario_media  salario_mediana\n","0      Ana   20000.0       20000.00          20000.0\n","1    Bruno       NaN       41666.67          20000.0\n","2   Carlos    5000.0        5000.00           5000.0\n","3  Daniele       NaN       41666.67          20000.0\n","4  Gustavo  100000.0      100000.00         100000.0"],"text/html":["\n","  <div id=\"df-c94a165b-51ab-4a5a-800a-d464d8c3a956\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>nome</th>\n","      <th>salario</th>\n","      <th>salario_media</th>\n","      <th>salario_mediana</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    